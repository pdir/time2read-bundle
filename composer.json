{
    "name": "pdir/time2read-bundle",
    "type": "contao-bundle",
    "description": "Diese Erweiterung ermittelt die Lesezeit eines News-Artikels und stellt diese im Frontend zur Verfügung.",
    "homepage": "https://contao.org",
    "license": "LGPL-3.0-or-later",
    "authors": [{
            "name": "ORÖ GmbH",
            "homepage": "https://oroe.de"
        },
        {
            "name": "Contao-Stammtisch Dresden",
            "homepage": "https://gondao.org"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "contao/core-bundle": "^4.4",
        "symfony/framework-bundle": "^3.3"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "Pdir\\TimeToReadBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Pdir\\TimeToReadBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pdir\\TimeToReadBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/pdir/time2read-bundle/issues",
        "source": "https://github.com/pdir/time2read-bundle"
    }
}
